# .path: prepend locations to $PATH
# Source: @webpro

# Prepend rbenv to PATH
export PATH="$HOME/.rbenv/bin:$PATH"

# Prepend GNU Core Utils from brew
export PATH=""$(brew --prefix coreutils)/libexec/gnubin":$PATH"

# Append Go binaries
export PATH="$PATH:$GOPATH/bin"
export PATH="$PATH:$GOROOT/bin"

# Figma: load local npm binaries
export PATH="$PATH:$HOME/npm/bin"

# Chromium: add depot_tools to path
export PATH="$PATH:$HOME/Work/chromium-dev/depot_tools"
